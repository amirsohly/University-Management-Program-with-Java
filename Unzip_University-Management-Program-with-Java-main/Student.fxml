<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.StudentController">
   <children>
      <Button fx:id="receivedClasses" alignment="CENTER" layoutX="396.0" layoutY="130.0" mnemonicParsing="false" onAction="#receivedClasses" prefHeight="82.0" prefWidth="190.0" text="Received Classes" textAlignment="CENTER">
         <font>
            <Font name="Nazanin" size="20.0" />
         </font>
      </Button>
      <Label fx:id="label" alignment="CENTER" cache="true" contentDisplay="CENTER" layoutX="180.0" layoutY="31.0" prefHeight="64.0" prefWidth="253.0" textFill="Green">
         <font>
            <Font name="Nazanin" size="20.0" />
         </font>
      </Label>
      <Button fx:id="getClassStu" alignment="CENTER" layoutX="14.0" layoutY="284.0" mnemonicParsing="false" onAction="#getClassStu" prefHeight="82.0" prefWidth="190.0" text="Get Class" textAlignment="CENTER">
         <font>
            <Font name="Nazanin" size="20.0" />
         </font>
      </Button>
      <Button fx:id="RemoveClass" alignment="CENTER" layoutX="396.0" layoutY="284.0" mnemonicParsing="false" onAction="#removeClass" prefHeight="82.0" prefWidth="190.0" text="Remove Class" textAlignment="CENTER">
         <font>
            <Font name="Nazanin" size="20.0" />
         </font>
      </Button>
      <Button fx:id="viewClasses1" alignment="CENTER" layoutX="14.0" layoutY="130.0" mnemonicParsing="false" onAction="#viewClasses" prefHeight="82.0" prefWidth="190.0" text="View All Classes" textAlignment="CENTER">
         <font>
            <Font name="Nazanin" size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
