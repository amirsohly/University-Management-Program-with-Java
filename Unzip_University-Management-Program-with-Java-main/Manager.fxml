<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ManagerController">
   <children>
      <Button fx:id="Present_Class_btn" alignment="CENTER" layoutX="205.0" layoutY="16.0" mnemonicParsing="false" onAction="#presentClass" prefHeight="82.0" prefWidth="190.0" text="Present Class" textAlignment="CENTER">
         <font>
            <Font name="Nazanin" size="20.0" />
         </font>
      </Button>
      <Button fx:id="Employ_Teacher_btn" alignment="CENTER" layoutX="205.0" layoutY="111.0" mnemonicParsing="false" onAction="#employTeacher" prefHeight="82.0" prefWidth="190.0" text="Employ Teacher" textAlignment="CENTER">
         <font>
            <Font name="Nazanin" size="20.0" />
         </font>
      </Button>
      <Button fx:id="Register_Student_btn" alignment="CENTER" layoutX="205.0" layoutY="207.0" mnemonicParsing="false" onAction="#registerStudent" prefHeight="82.0" prefWidth="190.0" text="Register Student" textAlignment="CENTER">
         <font>
            <Font name="Nazanin" size="20.0" />
         </font>
      </Button>
      <Button fx:id="View_Classes_btn" alignment="CENTER" layoutX="205.0" layoutY="305.0" mnemonicParsing="false" onAction="#viewClasses" prefHeight="82.0" prefWidth="190.0" text="View Classes" textAlignment="CENTER">
         <font>
            <Font name="Nazanin" size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
